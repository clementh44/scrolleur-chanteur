{
  "name": "@clementh44/scrolleur-chanteur",
  "version": "4.24.10",
  "description": "Pour faire \"scroller\" les chants pour... chanter",
  "engines": {
    "node": "^18"
  },
  "scripts": {
    "start": "http-server ./dist",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "sitemap": "vue-cli-service sitemap",
    "build-report": "vue-cli-service build --report",
    "check": "npx --yes oxlint@latest --deny-warnings && npm run lint && npm run build",
    "clean": "rm -rf dist",
    "maintenance": "npm run clean && find . -name 'node_modules' -type d -prune -exec rm -rf '{}' + && npm cache clean --force",
    "secure": "npx --yes @nodesecure/cli@2.4.0 cwd && npx @nodesecure/cli@2.4.0 open",
    "deps:update": "npx taze",
    "deps:unused": "npx knip@5.8.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/vue-fontawesome": "^2.0.10",
    "@tiptap/extension-blockquote": "^2.6.6",
    "@tiptap/extension-bullet-list": "^2.6.6",
    "@tiptap/extension-color": "^2.6.6",
    "@tiptap/extension-heading": "^2.6.6",
    "@tiptap/extension-horizontal-rule": "^2.6.6",
    "@tiptap/extension-list-item": "^2.6.6",
    "@tiptap/extension-ordered-list": "^2.6.6",
    "@tiptap/extension-text-align": "^2.6.6",
    "@tiptap/extension-text-style": "^2.6.6",
    "@tiptap/extension-typography": "^2.6.6",
    "@tiptap/extension-underline": "^2.6.6",
    "@tiptap/starter-kit": "^2.6.6",
    "@tiptap/vue-2": "^2.6.6",
    "axios": "^0.26.1",
    "bootstrap": "^4.6.2",
    "bootstrap-vue": "^2.23.1",
    "core-js": "^3.38.1",
    "esm": "^3.2.25",
    "vue": "^2.7.16",
    "vue-body-class": "^3.0.2",
    "vue-cookies": "^1.8.4",
    "vue-debounce": "^2.6.0",
    "vue-gtag": "^1.16.1",
    "vue-router": "^3.6.5",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^9.28.0",
    "http-server": "^14.1.1",
    "vue-cli-plugin-sitemap": "^2.3.0",
    "vue-template-compiler": "^2.7.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/clementh44/scrolleur-chanteur.git"
  }
}
